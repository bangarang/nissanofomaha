 <div class="photo_container">
  <h1>Listing photos</h1>

  <div id="photos">
    <%= render @photos %>
  </div>
  <div class="clear"></div>

  <%= s3_uploader_form post: photos_url, as: "photo[image_url]" do %>
    <%= file_field_tag :file, multiple: true %>
  <% end %>

  <script id="template-upload" type="text/x-tmpl">
    <div class="upload">
      {%=o.name%}
      <div class="progress">
        <div class="bar" style="width: 0%">
        </div>
      </div>
    </div>
  </script>

</div>