@font-face {
    font-family: 'nissan-ag';
    src: url('<%= asset_path('/assets/nissanag-light-webfont.eot') %>');
    src: url('<%= asset_path('/assets/nissanag-light-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('/assets/nissanag-light-webfont.woff') %>') format('woff'),
         url('<%= asset_path('/assets/nissanag-light-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('/assets/nissanag-light-webfont.svg#nissanaglight') %>') format('svg');
    font-weight: 200;
    font-style: normal;
}
